type Team = vec Player;
type Player = 
 record {
   id: nat;
   name: text;
   position: text;
   projectedPoints: float64;
   salary: nat;
   team: text;
 };
service : {
  deleteTeam: () -> (bool);
  getAvailablePlayers: () -> (vec Player) query;
  getTeam: () -> (opt Team) query;
  saveTeam: (vec Player) -> (bool);
}
